{
  "name": "next-level-routes",
  "version": "0.1.0",
  "description": "Next level routing for nextjs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nodemon",
    "build": "unbuild",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "prepack": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ifeanyi Isitor",
  "license": "MIT",
  "dependencies": {
    "esm": "^3.2.25",
    "inversify": "^6.0.1",
    "meow": "^12.0.1",
    "reflect-metadata": "^0.1.13",
    "unbuild": "^1.2.1"
  },
  "devDependencies": {
    "@types/esm": "^3.2.0",
    "@types/node": "^20.4.8",
    "eslint": "^8.46.0",
    "eslint-config-unjs": "^0.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "bin": {
    "nlr": "./dist/index.mjs"
  }
}
